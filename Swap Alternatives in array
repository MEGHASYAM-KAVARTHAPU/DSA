#include <iostream>

using namespace std;
void  reverse(int a[],int n){

int dummy;
    for(int i=0;i<n-1;i=i+2)
       { dummy=a[i];
        a[i]=a[i+1];
        a[i+1]=dummy;}
        //swap(a[start],a[end]);
       
    
    for(int i=0;i<n;i++){
        cout<<a[i];
    }
}

int main()
{ int arr[]={1,2,3,4,5,6,7,8,8,1};
   reverse(arr,10);
  
    return 0;
}
