#include <iostream>

using namespace std;
void bubbleSort(int arr[],int n){
    for(int i=1;i<n;i++){
        for(int j=0;j<n-i;j++){
            if(arr[j+1]<arr[j]){
                swap(arr[j+1],arr[j]);
            }
        }
    }
for(int i=0;i<n;i++){
    cout<<arr[i]<<endl;
}
}

int main()
{ int arr[]={4,2,1,9,0,6,3,8,4,3};
  bubbleSort(arr,10);
    

    return 0;
}
