
#include <iostream>

using namespace std;
void selectionSort(int arr[],int n){
    for(int i=0;i<n-1;i++){
        int minIndex=i;
        for(int j=i+1;j<n;j++){
            if(arr[minIndex]>arr[j]){
                minIndex=j;
                
            }
        }
         swap(arr[i],arr[minIndex]);
            }
    
    for(int i=0;i<n;i++){
        cout<<arr[i];
}}

int main()
{
    int k[]={2,1,9,5,6,3,4};
    selectionSort(k,7);

    return 0;
}
